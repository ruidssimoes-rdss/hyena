{
  "name": "@hyena-studio/react-native",
  "version": "0.1.0",
  "description": "Universal React Native component library with beautiful defaults",
  "homepage": "https://hyena.studio",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.mjs",
      "require": "./dist/tokens/index.js"
    },
    "./themes": {
      "types": "./dist/themes/index.d.ts",
      "import": "./dist/themes/index.mjs",
      "require": "./dist/themes/index.js"
    }
  },
  "scripts": {
    "build": "tsup && tsc -p tsconfig.build.json || true",
    "build:js": "tsup",
    "build:types": "tsc -p tsconfig.build.json || true",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.0.0",
    "expo-blur": ">=12.0.0",
    "nativewind": "^4.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.72.0",
    "react-native-safe-area-context": ">=4.0.0",
    "react-native-svg": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "@react-native-async-storage/async-storage": {
      "optional": true
    },
    "expo-blur": {
      "optional": true
    },
    "react-native-safe-area-context": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.45",
    "@types/react-native": "^0.72.8",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "react-test-renderer": "^18.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "react-native",
    "ui",
    "components",
    "nativewind",
    "tailwind",
    "dark-mode",
    "glassmorphism"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyena-studio/hyena"
  },
  "files": [
    "dist"
  ]
}
